
<div class="modal-header">
    <h3 class="modal-title">{{action}}</h3>
</div>
<div class="modal-body">
    Project Name <input type="text" ng-model="projectName"/>
    <br/>
    Task Name <input type="text" ng-model="taskName" />
    <br />
    Assign To <input type="text" ng-model="assignee"/>
    <br/>
    Start Time <input type="text" datepicker-popup="yyyy-MM-dd" is-open="isStartTimeCalanderOpened" ng-model="startTime" />
    <button ng-click="showStartTimeCalander($event)"><i class="glyphicon glyphicon-calendar"></i></button>
    <br/>
    End Time <input type="text" datepicker-popup="yyyy-MM-dd" is-open="isEndTimeCalanderOpened" ng-model="endTime" />
    <button ng-click="showEndTimeCalander($event)"><i class="glyphicon glyphicon-calendar"></i></button>
    <br/>
    Total Hours <input type="text" ng-model="totalHours">
    <br/>
    Hours Per Day <input type="text" ng-model="hoursPerDay">
</div>
<div class="modal-footer">
    <button class="btn btn-primary" ng-click="ok()">OK</button>
    <button class="btn btn-primary" ng-click="cancel()">Cancel</button>
</div>
